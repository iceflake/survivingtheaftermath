<data version="0.0.6">

<!-- Loc keys

#EVENT_FISH_FACTORY_01_NAME
#EVENT_FISH_FACTORY_01_DESC
#EVENT_FISH_FACTORY_01_NOTIFICATION_HEADER
#EVENT_FISH_FACTORY_01_NOTIFICATION_DESC
